<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Screening - smoke tests" parallel="classes" thread-count="2">
    <test name="Main functional">
        <groups>
            <run>
                <include name="smoke test"/>
            </run>
        </groups>
        <classes>
            <class name="common.SuiteTestPT"/>
            <class name="projects.com.swag.screening.authorization.RegistrationTest"/>
            <class name="projects.com.swag.screening.candidate.CandidateTest"/>
            <class name="projects.com.swag.screening.problem.ProblemTest"/>
            <class name="projects.com.swag.screening.candidate.CandidateFlowTest"/>
        </classes>
    </test>
</suite>
