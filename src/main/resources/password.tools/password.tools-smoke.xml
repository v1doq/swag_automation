<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Password tools - smoke tests" parallel="classes" thread-count="2">
    <test name="Main functional">
        <groups>
            <run>
                <include name="smoke test"/>
            </run>
        </groups>
        <classes>
            <class name="projects.com.password.tools.EmployeeTest"/>
            <class name="projects.com.password.tools.ResourceTest"/>
            <class name="projects.com.password.tools.CategoryTest"/>
            <class name="projects.com.password.tools.PropertyTest"/>
            <class name="projects.com.password.tools.ProfileTest"/>
        </classes>
    </test>
</suite>
